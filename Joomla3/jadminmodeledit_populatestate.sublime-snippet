	<content><![CDATA[/**
 * Stock method to auto-populate the model state.
 *
 * @return  void
 *
 * @since   $TM_VERSION
 */
protected function populateState()
{
	// Execute the parent method.
	parent::populateState();

	// Get the application.
	\$app = JFactory::getApplication();
}]]></content>
