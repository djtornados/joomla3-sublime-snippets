	<content><![CDATA[/**
 * Method to catch the onAfterDispatch event.
 *
 * @return  boolean  True on success.
 *
 * @since   $TM_VERSION
 */
public function onAfterDispatch()
{
	// Check that we are in the site application.
	if (JFactory::getApplication()->isAdmin())
	{
		return true;
	}

	//

	return true;
}]]></content>
